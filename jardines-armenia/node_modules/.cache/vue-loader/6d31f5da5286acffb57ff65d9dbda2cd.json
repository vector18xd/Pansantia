ceRoot":"src/views","sourcesContent":["<template>\r\n<div>\r\n    <div class=\"container\">\r\n      <h1 style=\"color:#12A19B;\">\r\n        <h3>Plan de Proteccion Multiplan</h3>\r\n      </h1>\r\n    </div>\r\n\r\n    <p style=\"font-size: 24px; color: #666\">\r\n      “Producto diseñado para que usted y su familia  integre un plan  según su necesidad de protección”\r\n    </p>\r\n\r\n    <div class=\"container\">\r\n        <div class=\"row\">\r\n      \r\n        <div class=\"column-5001\">\r\n          <h1 style=\"color:#12A19B;\">Destino Final</h1>\r\n          <p align=\" justify\" style=\"font-size: 18px\">\r\n            <br>\r\n            <span>\r\n              1. Cremación en Jardines de Armenia.\r\n              <br> \r\n              2. Lote por 4 años en Jardines de Armenia Sector 13 A.\r\n              <br>\r\n              3. Lote  sector 13 a y/o cremación  en Jardines de Armenia.\r\n              <br>   \r\n              4. Lote preferencial sector 15.\r\n              <br>\r\n              5. Lote preferencial sector 15 y/o cremación Jardines de Armenia.\r\n              <br>\r\n              6. Lote y/o cremación  a nivel Nacional.\r\n            </span>\r\n          </p>\r\n        </div>\r\n     \r\n      <br>\r\n      <div class=\"column-5000\">\r\n          <h1 style=\"color:#12A19B;\">Servicio Inicial</h1>\r\n          <p align=\" justify\" style=\"font-size: 18px\">\r\n            <br>\r\n            <span>\r\n              1. Traslado  50 Km de ida y 50 km regreso.\r\n              <br>\r\n              2. Cofre funerario Estándar.\r\n              <br>\r\n              3. Preservación normal del cuerpo.\r\n              <br>\r\n              4. Velación por 24 horas.\r\n              <br> \r\n              5. Tramites legales.\r\n              <br>\r\n              6. Cafetería.\r\n              <br>\r\n              7. Arreglo floral.\r\n              <br>\r\n              8. Un cartel ( con diez nombres ).\r\n              <br>\r\n              9. Kit Funerario ( Libro de Asistencia, recordatorio, Bono Unidad del duelo, Novenario, Denario).\r\n              <br> \r\n              10. Juego de cirios.\r\n              <br>\r\n              11. Cinta membretada.\r\n              <br>\r\n              12. Bus acompañante.\r\n              <br>\r\n              13. Misa.\r\n            </span>\r\n          </p>\r\n      </div>\r\n     \r\n        <div class=\"column-5001\">\r\n          <h1 style=\"color:#12A19B;\">Beneficios Adicionales</h1>\r\n          <p align=\" justify\" style=\"font-size: 18px\">\r\n            <br>\r\n            <span>\r\n              1. Repatriación.\r\n              <br>\r\n              2. Seguro de vida.\r\n              <br>\r\n              3. Asomavi.\r\n              <br>\r\n              4. Protección de Mascota \r\n              <br>\r\n              5. Traslado Nacional.\r\n              <br>\r\n              6. Osario.\r\n              <br>\r\n              7. Cenizario.\r\n              <br>\r\n              8. Exhumación.\r\n              <br>\r\n              9. Cremación de restos.\r\n            </span>\r\n          </p>\r\n        </div>      \r\n      \r\n      <br>\r\n      \r\n      <br>\r\n    </div>\r\n    <br><br><br>\r\n    </div>\r\n</div>\r\n</template>\r\n\r\n<style>\r\n.column-5000 {\r\n  float: left;\r\n  width: 48%;\r\n  padding: 11px;\r\n}\r\n\r\n.column-5001 {\r\n  float: right;\r\n  width: 48%;\r\n  padding: 11px;\r\n}\r\n\r\n</style>"]}]}